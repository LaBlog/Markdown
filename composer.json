{
    "name": "lablog/markdown",
    "description": "Markdown processer filter for twig templating engine. Also includes laravel service provider to link to Lablog.",
    "authors": [
        {
            "name": "Joseph Ralph",
            "email": "joe@joescode.co.uk"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*",
        "twig/twig": "1.*",
        "michelf/php-markdown": "1.4.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Lablog\\Markdown\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
